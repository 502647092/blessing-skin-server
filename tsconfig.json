{
  "extends": "@gplane/tsconfig",
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./resources/assets/src/*"]
    }
  },
  "include": ["resources/assets/src", "resources/assets/webpack.d.ts"],
  "ts-node": {
    "compilerOptions": {
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "module": "commonjs"
    }
  }
}
