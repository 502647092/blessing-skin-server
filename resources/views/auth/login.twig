{% extends 'auth.base' %}

{% block title %}{{ trans('auth.login.title') }}{% endblock %}

{% block content %}
  {% for row in rows %}
    {{ include(row) }}
  {% endfor %}
{% endblock %}

{% block before_foot %}
  <script>
    Object.defineProperty(blessing, 'extra', {
      configurable: false,
      get: () => Object.freeze({{ extra|json_encode|raw }})
    })
  </script>
{% endblock %}
